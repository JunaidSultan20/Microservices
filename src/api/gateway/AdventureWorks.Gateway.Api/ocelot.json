{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/customer?{everything}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "sales.api",
          "Port": 8081
        }
      ],
      "UpstreamPathTemplate": "/api/customer?{everything}",
      "UpstreamHttpMethod": [ "GET" ],
      "DangerousAcceptAnyServerCertificateValidator": true,
      "UpstreamHeaderTransform": {
        "X-Forwarded-For": "{BaseUrl}"
      },
      "DownstreamHeaderTransform": {
        "X-Forwarded-For": "{BaseUrl}"
      },
      "RateLimitOptions": {
        "EnableRateLimiting": true,
        "Period": "10s",
        "PeriodTimespan": 10,
        "Limit": 5
      },
      "SwaggerKey": "customer",
      "RouteIsCaseSensitive": false
    },
    {
      "DownstreamPathTemplate": "/api/account/login",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "identity.api",
          "Port": 8081
        }
      ],
      "UpstreamPathTemplate": "/api/account/login",
      "UpstreamHttpMethod": [ "POST" ],
      "DangerousAcceptAnyServerCertificateValidator": true,
      "UpstreamHeaderTransform": {
        "X-Forwarded-For": "{BaseUrl}"
      },
      "DownstreamHeaderTransform": {
        "X-Forwarded-For": "{BaseUrl}"
      },
      "RateLimitOptions": {
        "EnableRateLimiting": true,
        "Period": "10s",
        "PeriodTimespan": 10,
        "Limit": 3
      },
      "RouteIsCaseSensitive": false
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "localhost:6002"
    //"ServiceDiscoveryProvider": {
    //  "Host": "consul",
    //  "Port": 8500,
    //  "Type": "Consul"
    //}
  }
}